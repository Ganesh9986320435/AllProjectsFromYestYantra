<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
            http://www.springframework.org/schema/beans/spring-beans.xsd
            http://www.springframework.org/schema/context
            http://www.springframework.org/schema/context/spring-context-3.0.xsd">

	<bean id="item" class="springproject2.Items">
	<property name="name" value="biriyani"></property>
	<property name="price" value="200"></property>
	<property name="manufacturer" value="rajath"></property>
	<property name="qty" value="2"></property>
	
	
	
	</bean>
</beans>
