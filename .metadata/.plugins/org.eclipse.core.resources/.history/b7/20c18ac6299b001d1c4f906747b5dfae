
public class ex {
	public static void main(String[] args) {
		int a[] = {1,3,5,7};
		int b[] = {2,4,6,8};
		int c[]= new int [a.length+b.length];
		int k=0;
		for(int i=0;i<c.length;i++) {
			for (int j = 0; j < a.length; j++) {
				
				c[k++] = a[j];
				
			}
			for (int j = 0; j < b.length; j++) {
				c[i] = a[j];
				
			}
		}
		for (int j = 0; j < c.length; j++) {
			System.out.println(c[j]);
		}
	}
}
