
public class ex {
	public static void main(String[] args) {
	int a[]= {1,3,5,7};
	int b[]= {2,4,6,8};
	int c[]=new int[a.length+b.length];
		int j=0;
	 	for(int i=0;i<a.length;i++)
	 	{
	 		if(a[i]<b[i])
	 		{
	 			c[j]=a[i];
	 			c[++j]=b[i];
	 		}
	 		else{
	 			c[j]=b[i];
	 			c[j+1]=a[i];
	 		}
	 		
	 	}
	 	for(int i=0;i<a.length;i++)
	 	{
	 		System.out.println(c[i]);
	 	}
	}
}
