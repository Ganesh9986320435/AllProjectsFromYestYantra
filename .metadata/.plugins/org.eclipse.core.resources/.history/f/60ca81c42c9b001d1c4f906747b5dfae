
public class ex {
	public static void main(String[] args) {
	int a[]= {1,2,3,8};
	int b[]= {4,5,6,7};
	if(a[0])
	int c[]=new int[a.length+b.length];
		int j=0;
	 	for(int i=0;i<a.length;i++)
	 	{
	 		if(a[i]<b[i]&&b[i]<a[i+1])
	 		{
	 			c[j]=a[i];
	 			c[++j]=b[i];
	 		}
	 		else{
	 			c[j]=b[i];
	 			c[++j]=a[i];
	 		}j++;
	 		
	 	}
	 	for(int i=0;i<c.length;i++)
	 	{
	 		System.out.println(c[i]);
	 	}
	}
}
