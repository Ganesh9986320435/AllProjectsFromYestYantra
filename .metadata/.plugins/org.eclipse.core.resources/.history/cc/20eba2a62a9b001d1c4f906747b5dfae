
public class ex {
	public static void main(String[] args) {
	int a[]= {1,2,3,6};
	int b[]= {4,5,7,8};
	int c[]=new int[a.length+b.length];
	 	for(int i=0;i<a.length;i++)
	 	{
	 		for(int j=0;j<b.length;j++)
	 		{
	 			if(a[i]<b[i])
	 			{
	 				c[j]=a[i];
	 			}
	 		}
	 	}
	 	for(int i=0;i<a.length;i++)
	 	{
	 		System.out.println();
	 	}
	}
}
